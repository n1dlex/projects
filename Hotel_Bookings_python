        "# Average Daily Rate (ADR) trends\n",
        "df['month_year'] = pd.to_datetime(df['arrival_date_year'].astype(str) + '-' + df['arrival_date_month'].astype(str) + '-01')\n",
        "adr_trends = df.groupby('month_year')['adr'].mean().reset_index()\n",
        "\n",
        "fig = px.line(adr_trends, x='month_year', y='adr',\n",
        "              title='Average Daily Rate (ADR) Trends',\n",
        "              labels={'month_year': 'Date', 'adr': 'Average Daily Rate'})\n",
        "fig.show()\n"
